<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nfebrian13.repository.StudentRepository">

	<select id="findById" resultType="com.nfebrian13.model.Student">
		SELECT * FROM student WHERE id = #{id}
	</select>
	
	<select id="findAll" resultType="com.nfebrian13.model.Student">
		select * FROM student
	</select>

</mapper>